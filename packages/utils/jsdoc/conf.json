{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": ["./src"],
		"exclude": ["./node_modules"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"default": {
			"useLongnameInNav": true,
			"includeDate": false
		}
	},
	"opts": {
		"recurse": true,
		"theme_opts": {
			"default_theme": "light",
			"title": "@orz2/utils"
		},
		"template": "node_modules/clean-jsdoc-theme",
		"encoding": "utf8",
		"destination": "../../dist/sub-utils/"
	}
}
